    <Button fx:id="forgotPass" onAction="#forgotPassOnAction" prefWidth="245.0" text="Forgot Password?" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="250.0" />
   <Label layoutX="235.0" layoutY="22.0" prefHeight="59.0" prefWidth="130.0" text="Company">
