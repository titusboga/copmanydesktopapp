<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="386.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.register.RegistrationController">

    <TextField fx:id="newUsernameField" layoutX="44.0" layoutY="140.0" prefWidth="500.0" promptText="New username/email" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="140.0" />
    <PasswordField fx:id="newPasswordField" layoutX="44.0" layoutY="190.0" prefWidth="500.0" promptText="New password" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="190.0" />
    <PasswordField fx:id="confirmPasswordField" layoutX="44.0" layoutY="240.0" prefWidth="500.0" promptText="Confirm password" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="240.0" />

    <Button fx:id="registerButton" layoutX="44.0" layoutY="290.0" onAction="#registerOnAction" prefWidth="500.0" text="Register" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="290.0" />
    <Button fx:id="backButton" layoutX="44.0" layoutY="340.0" onAction="#backOnAction" prefWidth="500.0" text="Back to Login" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="340.0" />

    <Label layoutX="243.0" layoutY="14.0" prefHeight="59.0" prefWidth="130.0" text="Register">
        <font>
            <Font size="29.0" />
        </font>
    </Label>
   <TextField fx:id="emailField" layoutX="44.0" layoutY="140.0" prefWidth="500.0" promptText="Email" />
   <TextField fx:id="usernameField" layoutX="44.0" layoutY="94.0" prefWidth="500.0" promptText="Username" />

</AnchorPane>
